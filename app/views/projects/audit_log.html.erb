<!-- app/views/projects/audit_log.html.erb --><h1>Audit Log</h1>
<% if @versions.any? %>
  <table class="table">
    <thead>
      <tr>
        <th>Version</th>
        <th>Event</th>
        <th>Action</th>
        <th>Changed At</th>
      </tr>
    </thead>
    <tbody>
      <% @versions.each do |version| %>
        <tr>
          <td><%= version.index %></td>
          <td><%= version.event %></td>
          <td><%= version.item_type %></td> <!-- Represents the type of item being logged -->
          <td><%= version.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
  <p>No audit log entries found.</p>
<% end %>
